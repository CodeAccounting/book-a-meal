  "engines": {
    "node": "9.11.1"
  },
  "scripts": {
    "start": "node ./server/dist/app.js",
    "start:dev": "cross-env NODE_ENV=development npm-run-all --parallel start:server:dev start:client:dev",
    "start:server:dev": "nodemon --exec babel-node -- ./server/src/app.js",
    "start:client:dev": "webpack --config webpack.dev.js",
    "build": "npm-run-all build:server build:client",
    "build:server": "cross-env NODE_ENV=production babel ./server/src -d ./server/dist",
    "build:client": "webpack --config webpack.prod.js ",
    "heroku-postbuild": "npm run build",
    "pretest:server": "npm-run-all lint migrate:test seed:test",
    "lint": "eslint **/*.js --fix",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:test": "sequelize db:migrate:undo:all --env=test && sequelize db:migrate --env=test",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env=test",
    "test:e2e": "cross-env NODE_ENV=test nightwatch",
    "e2e:setup": "selenium-standalone install --config=./selenium.config.js",
    "coverage": "npm run merge:lcov && cat ./coverage/lcov.info | coveralls",
    "test": "npm-run-all test:server test:client",
    "test:server": "cross-env NODE_ENV=test SECRET=mytestsecret nyc mocha ./server/tests/test.js --recursive --timeout 50000 --exit",
    "test:server:watch": "npm run test:server --watch",
    "test:client": "cross-env SECRET=mytestsecret jest --env=jsdom --coverage",
    "test:client:watch": "jest --env=jsdom --watch",
    "merge:lcov": "cat ./coverage/lcov.*.info > ./coverage/lcov.info"
  },
