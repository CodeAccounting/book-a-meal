{
  "name": "book-a-meal",
  "version": "1.0.0",
  "main": "./server/dist/app.js",
  "repository": "git@github.com:iverenshaguy/book-a-meal.git",
  "author": "Iveren Shaguy <iverenshaguy@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "9.11.1"
  },
  "scripts": {
    "start": "node ./server/dist/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./server/src/app.js",
    "build": "cross-env NODE_ENV=production babel ./server/src -d ./server/dist",
    "heroku-postbuild": "npm run build",
    "pretest": "npm-run-all lint migrate:test seed:test",
    "lint": "eslint **/*.js --fix",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:test": "sequelize db:migrate:undo:all --env=test && sequelize db:migrate --env=test",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env=test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test SECRET=mytestsecret nyc mocha ./server/tests/test.js --recursive --timeout 50000 --exit",
    "test:watch": "npm test --watch"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "npm-run-all": "^4.1.2",
    "pg": "7.4.1",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
  }
}
