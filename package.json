{
  "name": "book-a-meal",
  "version": "1.0.0",
  "main": "./server/dist/app.js",
  "repository": "git@github.com:iverenshaguy/book-a-meal.git",
  "author": "Iveren Shaguy <iverenshaguy@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "9.11.1"
  },
  "scripts": {
    "start": "node ./server/dist/app.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/src/app.js",
    "build": "cross-env NODE_ENV=development babel ./server/src -d ./server/dist",
    "pretest": "",
    "lint": "eslint **/*.js --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc mocha server/tests/test.js --recursive --timeout 3000 --exit",
    "test:watch": "npm test --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "morgan": "^1.9.0",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "supertest": "^3.0.0"
  }
}
